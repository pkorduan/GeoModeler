<h1>Editing package</h1>

<% form_for(@package) do |f| %>
  <%= f.error_messages %>
  <p>
    <%= f.label :xmi_id %><br />
    <%= f.text_field :xmi_id %>
  </p>
  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :visibility %><br />
    <%= f.text_field :visibility %>
  </p>
  <p>
    <%= f.label :isSpecification %><br />
    <%= f.check_box :isSpecification %>
  </p>
  <p>
    <%= f.label :isRoot %><br />
    <%= f.check_box :isRoot %>
  </p>
  <p>
    <%= f.label :isLeaf %><br />
    <%= f.check_box :isLeaf %>
  </p>
  <p>
    <%= f.label :isAbstract %><br />
    <%= f.check_box :isAbstract %>
  </p>
  <p>
    <%= f.label :parent_package_id %><br />
    <%= f.text_field :parent_package_id %>
  </p>
  <p>
    <%= f.label :model_id %><br />
    <%= f.text_field :model_id %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @package %> |
<%= link_to 'Back', packages_path %>
<h1>Editing model</h1>

<% form_for(@model) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :xml_file_name %><br />
    <%= f.text_field :xml_file_name %>
  </p>
  <p>
    <%= f.label :owner_name %><br />
    <%= f.text_field :owner_name %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @model %> |
<%= link_to 'Back', models_path %>
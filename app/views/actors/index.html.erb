<h1>Listing actors</h1>

<table>
  <tr>
    <th>Xmi</th>
    <th>Model</th>
    <th>Name</th>
    <th>Package</th>
    <th>Visibility</th>
    <th>Isspecification</th>
    <th>Isroot</th>
    <th>Isleaf</th>
    <th>Isabstract</th>
  </tr>

<% @actors.each do |actor| %>
  <tr>
    <td><%=h actor.xmi_id %></td>
    <td><%=h actor.model_id %></td>
    <td><%=h actor.name %></td>
    <td><%=h actor.package_id %></td>
    <td><%=h actor.visibility %></td>
    <td><%=h actor.isSpecification %></td>
    <td><%=h actor.isRoot %></td>
    <td><%=h actor.isLeaf %></td>
    <td><%=h actor.isAbstract %></td>
    <td><%= link_to 'Show', actor %></td>
    <td><%= link_to 'Edit', edit_actor_path(actor) %></td>
    <td><%= link_to 'Destroy', actor, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New actor', new_actor_path %>
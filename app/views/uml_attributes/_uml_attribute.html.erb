<div id="<%= uml_attribute.xmi_id %>" alt="Attribut" class="content" name="<%= uml_attribute.name %>"  style="margin-left:30px;display:none;">  <%= image_tag("icons/empty_icon.png", :size => "11x11") %>	<%= image_tag("icons/#{uml_attribute.class.name}.png", :onclick => "toggleSelected($('#{uml_attribute.xmi_id}'))") %> <%= uml_attribute.name %>	<%= link_to_remote image_tag("icons/table.png"), :update => "#{uml_attribute.xmi_id}_data", :url => uml_attribute_url(:id => uml_attribute.id), :method => :get, :complete => "$('#{uml_attribute.xmi_id}_data').toggle();"; %><br>	<div id="<%= uml_attribute.xmi_id %>_data" class="data" style="display:none;"></div></div>